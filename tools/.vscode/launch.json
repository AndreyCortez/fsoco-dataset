    "version": "0.2.0",
    "configurations": [
        {
            "name": "NN Detect",
            "type": "python",
            "request": "launch",
            "program": "click_fsoco.py",
            "console": "integratedTerminal",
            "args": [
                "nn",
                "detect",
                "test",
                "test",
                "--test_split",
                "0.2",
                "--test"
            ]
        },
        {
            "name": "NN Train",
            "type": "python",
            "request": "launch",
            "program": "click_fsoco.py",
            "console": "integratedTerminal",
            "args": [
                "nn",
                "train",
                "${env:FSOCO_DATA}/fsoco_bounding_boxes_train",
                "${env:FSOCO_DATA}/fsoco_bounding_boxes_test",
                "work_dir",
                "--data",
                "data.yaml",
                "--weights",
                "yolov5s.pt",
                "nope"
            ]
        }
    ]
}